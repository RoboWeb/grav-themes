{
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/user/themes/quark/css-compiled/"
        },
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": "/user/themes/quark/css-compiled/"
        }
    ],
    "liveSassCompile.settings.excludeList": [
        "**/node_modules/**",
        ".vscode/**"
    ],
    "liveSassCompile.settings.includeItems": [
        "./user/themes/quark/scss/*.scss"
    ]
}